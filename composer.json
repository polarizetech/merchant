{
  "name": "polarizetech/merchant",
  "description": "Marketplace payments support for Laravel (via Stripe Connect)",
  "version": "0.0.3",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.2"
  },
  "autoload": {
      "psr-4": {
          "Merchant\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Merchant\\Tests\\": "tests"
      }
  },
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Merchant\\MerchantServiceProvider"
          ]
      }
  }
}
